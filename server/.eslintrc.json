{
  "extends": "standard",
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "ignore",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }],
    "no-extra-semi": "off",
    "semi": ["error", "always"]
  }
}


