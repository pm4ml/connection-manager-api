{
  "name": "connection-manager-api",
  "version": "1.7.16",
  "description": "ModusBox Connection Manager API",
  "main": "index.js",
  "scripts": {
    "testX": "standardx --verbose | snazzy && mocha",
    "test": "./run-vault.sh && mocha ; ./kill-vault.sh",
    "test-self-signed-server": "./run-vault.sh && TEST_START_SELF_SIGNED_SERVER=true npm test ; ./kill-vault.sh",
    "wtftest": "wtfnode ./node_modules/.bin/_mocha",
    "test-with-coverage": "./run-vault.sh && nyc --reporter=text mocha --timeout 20000 --colors -exit ; ./kill-vault.sh",
    "start": "node src/index.js",
    "start-with-oauth-enabled": "source oauth.env && node src/index.js",
    "start-for-tips": "source ../localEnvs/tips.env  && node src/index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js",
    "migrate": "DOTENV_CONFIG_PATH=$(pwd)/.env npx knex --knexfile src/knexfile.js migrate:latest",
    "seed": "DOTENV_CONFIG_PATH=$(pwd)/.env npx knex --knexfile src/knexfile.js seed:run",
    "migrate-and-seed": "npm -s run migrate && npm -s run seed"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "async-retry": "^1.3.1",
    "chai": "^4.2.0",
    "connect": "^3.2.0",
    "cookies": "^0.7.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "env-var": "^7.1.1",
    "express-winston": "^3.1.0",
    "form-data": "^2.3.3",
    "joi": "^14.3.1",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.21",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "node-forge": "^1.0.0",
    "node-vault": "^0.9.22",
    "oas3-tools": "^2.2.3",
    "objection": "^1.6.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sinon": "^7.4.1",
    "snazzy": "^8.0.0",
    "soap": "^0.35.0",
    "sqlite3": "^5.0.2",
    "swagger-tools": "^0.10.4",
    "tmp-promise": "^1.0.5",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jshint": "^2.13.1",
    "mocha": "^8.2.1",
    "nyc": "^14.1.1",
    "standardx": "^7.0.0"
  },
  "standardx": {
    "semistandard": true
  }
}
