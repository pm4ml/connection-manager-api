{
  "name": "@pm4ml/connection-manager-api",
  "version": "3.0.0",
  "description": "ModusBox Connection Manager API",
  "license": "Apache-2.0",
  "author": "ModusBox",
  "contributors": [
    "@Yevhen Kyriukha <yevhen.kyriukha@modusbox.com>",
    "Miguel de Barros <miguel.debarros@modusbox.com>",
    "Juan Correa <juan.correa@modusbox.com>"
  ],
  "main": "src/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepare": "husky || true",
    "test": "npm run test:int",
    "test:int": "echo 'WARNING: Run \"npm run backend:start\" before executing...\n' && TEST=true TEST_INT=true SWITCH_ID=switch jest --forceExit --runInBand --testMatch '**/test/int/**/*.test.js'",
    "test-self-signed-server": "TEST_START_SELF_SIGNED_SERVER=true jest --testMatch '**/test/int/**/*self.test.js'",
    "test:unit": "jest --testMatch '**/test/unit/**/*.test.js'",
    "test:xunit": "npm run test:unit",
    "test-with-coverage": "./docker compose up -d --wait vault-dev && nyc --reporter=text jest --timeout 20000 --colors -exit ; docker compose down -v",
    "test:coverage": "NODE_ENV=test DOTENV_CONFIG_PATH=.env.test nyc --clean jest --require dotenv/config --testMatch '**/test/**/*.test.js' --timeout 20000",
    "test:coverage-check": "true",
    "test:integration": "./test/scripts/test-integration.sh",
    "test:functional": "./scripts/run-functional-tests.sh",
    "precoverage": "rm -rf .nyc_output coverage",
    "lint": "npx eslint ./src ./test/int",
    "lint:fix": "npm run lint -- --fix",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf ./dist",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "release": "npx standard-version --no-verify --releaseCommitMessageFormat 'ci(release): {{currentTag}} [skip ci]'",
    "snapshot": "npx standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'ci(snapshot): {{currentTag}}'",
    "start": "node src/index.js",
    "start:dev": "node -r dotenv/config src/index.js",
    "start-with-oauth-enabled": "source oauth.env && node src/index.js",
    "migrate": "npm run knex-cli  migrate:latest",
    "seed": "npm run knex-cli  seed:run",
    "migrate-and-seed": "npm run migrate  &&  npm run seed",
    "migrate:rollback": "npm run knex-cli  migrate:rollback",
    "migrate:new": "npm run knex-cli  migrate:make",
    "knex-cli": "DOTENV_CONFIG_PATH=$(pwd)/.env npx knex --knexfile src/knexfile.js ",
    "backend:start": "docker compose up -d --wait && npm run migrate-and-seed",
    "backend:start-functional": "docker compose --profile functional up -d --wait && npm run migrate-and-seed",
    "backend:stop": "docker compose down -v",
    "backend:restart": "npm run backend:stop && npm run backend:start"
  },
  "keywords": [
    "swagger"
  ],
  "overrides": {
    "cookie": "1.0.2",
    "multer": "^2.0.0",
    "nanoid": "5.1.5",
    "ejs": "3.1.10",
    "sanitize-html": "2.12.1",
    "postcss": {
      "nanoid": "5.1.5"
    },
    "shins": {
      "ejs": "3.1.10",
      "sanitize-html": "2.12.1",
      "jsonpointer": "5.0.0",
      "markdown-it": "12.3.2",
      "postcss": "8.4.31"
    },
    "widdershins": {
      "markdown-it": "12.3.2",
      "swagger2openapi": "7.0.8"
    },
    "markdown-it": "12.3.2",
    "trim": "0.0.3",
    "cross-spawn": "7.0.6",
    "yargs-parser": "21.1.1"
  },
  "dependencies": {
    "@hapi/hapi": "21.4.0",
    "@keycloak/keycloak-admin-client": "^26.2.5",
    "@kubernetes/client-node": "^1.3.0",
    "@mojaloop/central-services-logger": "11.9.0",
    "@mojaloop/central-services-metrics": "12.6.0",
    "@mojaloop/central-services-shared": "18.28.2",
    "async-exit-hook": "^2.0.1",
    "async-retry": "^1.3.3",
    "axios": "1.10.0",
    "body-parser": "^2.2.0",
    "connect": "^3.7.0",
    "convict": "6.2.4",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "env-var": "^7.5.0",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.1",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.3",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "mysql2": "^3.14.1",
    "node-forge": "^1.3.1",
    "node-vault": "^0.10.5",
    "oas3-tools": "^2.2.3",
    "openid-client": "^6.5.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "qs": "^6.14.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "soap": "^1.1.12",
    "ulid": "3.0.1",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "30.0.0",
    "audit-ci": "^7.1.0",
    "axios-mock-adapter": "2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jest-extended": "^6.0.0",
    "npm-check-updates": "^18.0.1",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "snazzy": "^9.0.0",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "standardx": "^7.0.0",
    "supertest": "^7.1.1",
    "tap-xunit": "^2.4.1",
    "tmp": "^0.2.3"
  },
  "standardx": {
    "semistandard": true
  }
}
