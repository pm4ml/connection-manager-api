{
  "name": "@pm4ml/connection-manager-api",
  "version": "2.9.3",
  "description": "ModusBox Connection Manager API",
  "license": "Apache-2.0",
  "author": "ModusBox",
  "contributors": [
    "@Yevhen Kyriukha <yevhen.kyriukha@modusbox.com>",
    "Miguel de Barros <miguel.debarros@modusbox.com>",
    "Juan Correa <juan.correa@modusbox.com>"
  ],
  "main": "src/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepare": "husky || true",
    "test": "npm run test:int",
    "test:int": "echo 'WARNING: Run \"npm run backend:start\" before executing...\n' && TEST=true SWITCH_ID=switch mocha './test/int/**/*.test.js'",
    "test-self-signed-server": "./run-vault.sh && TEST_START_SELF_SIGNED_SERVER=true npm test ; ./kill-vault.sh",
    "test:unit": "jest",
    "test:xunit": "npm run test:unit",
    "test-with-coverage": "./docker compose up -d --wait vault-dev && nyc --reporter=text mocha --timeout 20000 --colors -exit ; docker compose down -v",
    "test:coverage": "NODE_ENV=test DOTENV_CONFIG_PATH=.env.test nyc --clean mocha --require dotenv/config 'test/**/*.test.js' --timeout 20000",
    "test:coverage-check": "true",
    "test:integration": "true",
    "test:functional": "true",
    "precoverage": "rm -rf .nyc_output coverage",
    "lint": "npx eslint ./src ./test/int",
    "lint:fix": "npm run lint -- --fix",
    "audit:resolve": "npx resolve-audit --production",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf ./dist",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "release": "npx standard-version --no-verify --releaseCommitMessageFormat 'ci(release): {{currentTag}} [skip ci]'",
    "snapshot": "npx standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'ci(snapshot): {{currentTag}}'",
    "start": "node src/index.js",
    "start:dev": "node -r dotenv/config src/index.js",
    "start-with-oauth-enabled": "source oauth.env && node src/index.js",
    "migrate": "npm run knex-cli  migrate:latest",
    "seed": "npm run knex-cli  seed:run",
    "migrate-and-seed": "npm -s run migrate && npm -s run seed",
    "migrate:rollback": "npm run knex-cli  migrate:rollback",
    "migrate:new": "npm run knex-cli  migrate:make",
    "knex-cli": "NODE_OPTIONS='-r dotenv/config' npx knex --debug --knexfile src/knexfile.js ",
    "backend:start": "docker compose up -d --wait && npm run migrate-and-seed",
    "backend:stop": "docker compose down -v",
    "backend:restart": "npm run backend:stop && npm run backend:start"
  },
  "keywords": [
    "swagger"
  ],
  "overrides": {
    "cookie": "1.0.2"
  },
  "dependencies": {
    "@hapi/hapi": "21.4.0",
    "@kubernetes/client-node": "^1.2.0",
    "@mojaloop/central-services-logger": "11.9.0",
    "@mojaloop/central-services-metrics": "12.6.0",
    "async-exit-hook": "^2.0.1",
    "async-retry": "^1.3.3",
    "axios": "1.9.0",
    "body-parser": "^2.2.0",
    "connect": "^3.7.0",
    "convict": "6.2.4",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "env-var": "^7.5.0",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.2",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "mysql2": "^3.14.1",
    "node-forge": "^1.3.1",
    "node-vault": "^0.10.2",
    "oas3-tools": "^2.2.3",
    "openid-client": "^5.4.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "qs": "^6.14.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "soap": "^1.1.11",
    "ulid": "3.0.0",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "29.5.14",
    "axios-mock-adapter": "2.1.0",
    "chai": "^4.4.1",
    "chai-datetime": "^1.8.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "mocha": "^11.4.0",
    "npm-audit-resolver": "3.0.0-RC.0",
    "npm-check-updates": "^18.0.1",
    "nyc": "^17.1.0",
    "sinon": "^20.0.0",
    "snazzy": "^9.0.0",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "standardx": "^7.0.0",
    "supertest": "^7.1.1",
    "tap-xunit": "^2.4.1"
  },
  "standardx": {
    "semistandard": true
  }
}
