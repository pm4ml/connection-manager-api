PORT=3001
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_USER=mcm
DATABASE_PASSWORD=mcm
DATABASE_SCHEMA=mcm
VAULT_ENDPOINT=http://localhost:8233
VAULT_AUTH_METHOD=APP_ROLE
VAULT_ROLE_ID_FILE=./docker/vault/tmp/role-id
VAULT_ROLE_SECRET_ID_FILE=./docker/vault/tmp/secret-id
VAULT_PKI_CLIENT_ROLE=example.com
VAULT_PKI_SERVER_ROLE=example.com
SWITCH_ID=your_switch_id_value

# Keycloak integration settings
KEYCLOAK_ENABLED=false
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_DISCOVERY_URL=http://localhost:8080/realms/dfsps/.well-known/openid-configuration
KEYCLOAK_ADMIN_CLIENT_ID=connection-manager-client
KEYCLOAK_ADMIN_CLIENT_SECRET=client-secret
KEYCLOAK_DFSPS_REALM=dfsps
KEYCLOAK_AUTO_CREATE_ACCOUNTS=true

# OpenID Connect authentication settings
OPENID_ENABLED=false
OPENID_DISCOVERY_URL=https://your-auth-provider/.well-known/openid-configuration
OPENID_CLIENT_ID=connection-manager-api
OPENID_CLIENT_SECRET=your-client-secret
OPENID_REDIRECT_URI=http://localhost:3001/api/auth/callback
OPENID_JWT_COOKIE_NAME=MCM-API_ACCESS_TOKEN
OPENID_EVERYONE_ROLE=everyone
OPENID_MTA_ROLE=mta
OPENID_PTA_ROLE=pta

# 2FA Authentication settings
OPENID_ENABLE_2FA=false

# Session store settings
SESSION_STORE_SECRET=your_session_store_secret
